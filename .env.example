# Example .env for icka_keepalive.py

# Multi-account mode:
ICKA_ACCOUNTS_FILE=accounts.txt

# Single account mode (if you don't use ACCOUNTS_FILE)
# ICKA_EMAIL=example@example.com
# ICKA_PASSWORD=changeme

# Whether to loop forever (for direct nohup mode):
ICKA_FOREVER=false

# Sleep interval for forever mode:
ICKA_SLEEP_INTERVAL=1h

# Optional:
ICKA_USER_AGENT=Mozilla/5.0 (X11; Linux x86_64)
ICKA_LOG_LEVEL=INFO
